<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="222.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.squiddemo.controller.StageModalController">


    <children>
      <Label text="Id" />
      <TextField fx:id="txtId" />
        <Label text="Nama Peserta" />
        <TextField fx:id="txtNama" />
        <Label text="Umur Peserta" />
        <TextField fx:id="txtUmur" />
        <Label text="Keahlian Peserta" />
        <TextField fx:id="txtKeahlian">
            <VBox.margin>
                <Insets bottom="10.0" />
            </VBox.margin>
        </TextField>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <Button fx:id="btnok" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#shwok" text="OK" HBox.hgrow="ALWAYS" />
                <Button fx:id="btncancel" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#shwcancel" text="CANCEL" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
